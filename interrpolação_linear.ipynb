      "source": [
        "x0, y0 = 1.0, 1.5\n",
        "x1, y1 = 1.5, 2.0\n",
        "\n",
        "def interpolar(x, x0, y0, x1, y1):\n",
        "    return y0 + ((y1 - y0) / (x1 - x0)) * (x - x0)\n",
        "\n",
        "x = 1.3\n",
        "resultado = interpolar(x, x0, y0, x1, y1)\n",
        "print(f\"P1({x}) = {resultado}\")\n"
      ]
    }
  ]
